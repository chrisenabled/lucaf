{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../common/assets/images/liftupcare.ico"
    }
  },
  "initial": {
    "./middleware/requestLog": {},
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": [
        "deny"
      ]
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": [
      {
        "name": "client-files",
        "params": "$!../client"
      },
      {
        "name": "server-bootstrap-js",
        "paths": ["/server-assets/bootstrap-js"],
        "params": "$!../node_modules/bootstrap/dist/js"
      },
      {
        "name": "server-bootstrap-css",
        "paths": ["/server-assets/bootstrap-css"],
        "params":  "$!../node_modules/bootstrap/dist/css"
      },
      {
        "name": "server-jquery",
        "paths": ["/server-assets/jquery"],
        "params":  "$!../node_modules/jquery/dist"
      },
      {
        "name": "server-assets-css",
        "paths": ["/server-assets/css"],
        "params":  "$!../server/assets/css"
      },
      {
        "name": "server-assets-js",
        "paths": ["/server-assets/js"],
        "params":  "$!../server/assets/js"
      },
      {
        "name": "common-assets",
        "paths": ["/common-assets"],
        "params":  "$!../common/assets"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
