{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "lucafPostgres"
  },
  "AccessToken": {
    "dataSource": "lucafPostgres",
    "public": false
  },
  "ACL": {
    "dataSource": "lucafPostgres",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "lucafPostgres",
    "public": false
  },
  "Role": {
    "dataSource": "lucafPostgres",
    "public": false
  },
  "Work": {
    "dataSource": "lucafPostgres",
    "public": true,
    "$promise": {},
    "$resolved": true
  },
  "Project": {
    "dataSource": "lucafPostgres",
    "public": true,
    "$promise": {},
    "$resolved": true
  },
  "Photo": {
    "dataSource": "lucafPostgres",
    "public": true,
    "$promise": {},
    "$resolved": true
  },
  "Email": {
    "dataSource": "emailDs"
  },
  "Admin": {
    "dataSource": "lucafPostgres",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  }
}
